program relations_test

global int x
global int y
global byte a

begin
    x = 124500
    y = 32
    a = x < y
    put "Result of x < y: ", a+48, 13
    a = x <= y
    put "Result of x <= y: ", a+48, 13
    a = x > y
    put "Result of x > y: ", a+48, 13
    a = x >= y
    put "Result of x >= y: ", a+48, 13
end
